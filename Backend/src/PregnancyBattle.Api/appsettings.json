{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Npgsql": "Warning",
      "Npgsql.Command": "Information",
      "Npgsql.Connection": "Information",
      "PregnancyBattle.Infrastructure.Data": "Information",
      "PregnancyBattle.Infrastructure.Data.Repositories": "Information",
      "PregnancyBattle.Infrastructure.Data.Contexts": "Information",
      "PregnancyBattle.Api.Middlewares": "Information",
      "PregnancyBattle.Api.Controllers": "Information",
      "PregnancyBattle.Application.Services": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "YOUR_DATABASE_CONNECTION_STRING_HERE"
  },
  "Jwt": {
    "Key": "YOUR_JWT_SECRET_KEY_HERE",
    "Issuer": "PregnancyBattle",
    "Audience": "PregnancyBattleApp",
    "ExpiryInMinutes": 60
  },
  "Supabase": {
    "Url": "YOUR_SUPABASE_URL_HERE",
    "AnonKey": "YOUR_SUPABASE_ANON_KEY_HERE"
  },
  "DeepSeek": {
    "ApiKey": "YOUR_DEEPSEEK_API_KEY_HERE",
    "BaseUrl": "https://api.deepseek.com/v1",
    "Model": "deepseek-chat",
    "Enabled": true,
    "TimeoutSeconds": 30,
    "MaxRetries": 3
  },
  "TencentCos": {
    "SecretId": "YOUR_SECRET_ID_HERE",
    "SecretKey": "YOUR_SECRET_KEY_HERE",
    "BucketName": "YOUR_BUCKET_NAME_HERE",
    "Region": "YOUR_REGION_HERE",
    "BaseUrl": ""
  },
  "LocalStorage": {
    "UseAsBackup": true,
    "BasePath": "uploads",
    "BaseUrl": "YOUR_BASE_URL_HERE"
  }
}